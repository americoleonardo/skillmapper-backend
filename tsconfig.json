{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@user/*": ["./src/user/*"],
      "@skill/*": ["./src/skill/*"],
      "@test/*": ["./test/*"]
    },
    "incremental": true
  }
}
